<div id="board">
  <table>
    <% board.to_array.each do |row| %>
      <tr>
        <% row.each do |spot| %>
          <td>
            <%= content_tag(:div, "", id: spot.first['name'], class: spot.first['direction']) if spot.first %>
          </td>
        <% end %>
      </tr>
    <% end %>
  </table>
</div>
